# Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to version control

# Required API Configuration
VITE_API_BASE_URL=https://your-api-domain.com
VITE_X_API_KEY=your-api-key-here
VITE_X_API_SECRET=your-api-secret-here

# Required Azure Storage Configuration
VITE_AZURE_STORAGE_URL=https://yourstorage.blob.core.windows.net
VITE_AZURE_CONTAINER_NAME=your-container-name
